# Uncomment the next line to define a global platform for your project

source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_frameworks!
target 'WeatherGift' do
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'GoogleMaps'
    pod 'Alamofire', '~> 5.0.0-rc.3'
    pod 'SwiftyJSON', '~> 4.0'
end
